local LocalPlayer = game:GetService("Players").LocalPlayer
local TweenService = game:GetService("TweenService")
local SlashCommandsFrame = script.Parent.SlashCommandsFrame
local BannedFrame = script.Parent.BannedFrame
local TextBox = script.Parent.Insides.BottomFrame.TextBoxParent
local Chats = script.Parent.Insides.Chats
local MessageTemplate = Chats.MessageTemplate
local isUserScrolling = false

local function HWID()
	return "skibidi"
end

local function isAtBottom()
	return Chats.CanvasPosition.Y >= Chats.AbsoluteCanvasSize.Y - Chats.AbsoluteWindowSize.Y - 10
end

local function scrollToBottom()
	if not isUserScrolling then
		Chats.CanvasPosition = Vector2.new(0, Chats.AbsoluteCanvasSize.Y - Chats.AbsoluteWindowSize.Y)
	end
end

Chats:GetPropertyChangedSignal("CanvasPosition"):Connect(function()
	isUserScrolling = true
	task.wait(0.5)
	isUserScrolling = false
end)

local function NewSlashCommand(CommandNameAndArgs, Description)
	local NewCommand = SlashCommandsFrame.ScrollingFrame.SlashCommandTemplate:Clone()

	NewCommand.Name = string.split(CommandNameAndArgs, " ")[1]
	NewCommand.Parent = SlashCommandsFrame.ScrollingFrame
	NewCommand.Visible = true
	NewCommand.Text = '<b>'..CommandNameAndArgs..'</b>\n<font size="9">'..Description..'</font>'
	NewCommand.MouseButton1Down:Connect(function()
		TextBox.TextBox.Text = string.split(CommandNameAndArgs, " ")[1].." "
		wait(0.1)
		SlashCommandsFrame.Visible = false
	end)
end

NewSlashCommand("/copymessage [MessageId]", "copy a message")
NewSlashCommand("/copyusername [MessageId]", "copy a username in a message")
NewSlashCommand("/delete [MessageId]", "delete a message")
NewSlashCommand("/edit [MessageId] [Message]", "edit a message")
NewSlashCommand("/allowlinks", "make members have the ability to send links")
NewSlashCommand("/forbidlinks", "make members have no ability to send links")
NewSlashCommand("/listcensoredwords", "returns censored words")
NewSlashCommand("/censorword [Word]", "add a word that members cant say")
NewSlashCommand("/uncensorword [Word]", "make a word uncensored that was in the censored list")
NewSlashCommand("/lockdown", "make everyone not able to send messages")
NewSlashCommand("/unlockdown", "make everyone able to send messages after lockdown")
NewSlashCommand("/listtimeouts", "returns a list of users who are in a timeout")
NewSlashCommand("/timeout [Username] [Seconds]", "timeout a user for seconds (not more than 3 days)")
NewSlashCommand("/untimeout [Username]", "removes a timeout to a user")
NewSlashCommand("/listbanned", "returns a list of users who are banned")
NewSlashCommand("/ban [Username]", "ban a user")
NewSlashCommand("/unban [Username]", "unban a user")

TextBox.TextBox:GetPropertyChangedSignal("Text"):Connect(function()
	local TextTable = string.split(TextBox.TextBox.ContentText)
	local NoResults = true

	if string.find(TextTable[1], '/') then
		print("founf")
		local Position1, Position2 = string.find(TextTable[1], '/')

		if Position1 == 1 and Position2 == 1 then
			SlashCommandsFrame.Visible = true

			for i, v in SlashCommandsFrame.ScrollingFrame:GetChildren() do
				if v:IsA("TextButton") and v.Name ~= "SlashCommandTemplate" then
					if string.find(string.split(v.Text, "<b>")[2], TextTable[1]) then
						v.Visible = true
						NoResults = false
					else
						v.Visible = false
					end
				end
			end
		else
			SlashCommandsFrame.Visible = false
		end
	else
		SlashCommandsFrame.Visible = false
	end

	if NoResults == true then
		SlashCommandsFrame.Visible = false
	end
end)

local function TextContentToShow(Table)
	local Message = Table.Message
	local Username = Table.Username
	local DisplayName = Table.DisplayName
	local UNIXTimestamp = Table.UNIXTimestamp
	local MessageId = Table.MessageId
	local IsTemporary = Table.IsTemporary
	local IsClient = Table.IsClient
	local ContentText = ""

	if _G.LiveChatServer.VerifiedPeople[Username] then
		ContentText = utf8.char("0xE000")..' <b>'..DisplayName..'</b> <font size="8">(@'..Username..') '..os.date("%m/%d/%Y %I:%M %p", UNIXTimestamp)
	else
		ContentText = '<b>'..DisplayName..'</b> <font size="8">(@'..Username..') '..os.date("%m/%d/%Y %I:%M %p", UNIXTimestamp)
	end

	local NewMessage = ""
	for _, Word in string.split(Message, " ") do
		local LowWord = string.lower(Word)

		if string.find(LowWord, "@") then
			NewMessage = NewMessage..'<b><mark color="#31304f" transparency="0"><font color="#9db0f1">'..Word..'</font></mark></b> '
		else
			NewMessage = NewMessage..""..Word.." "
		end
	end

	Message = NewMessage

	if MessageId == nil then
		ContentText = ContentText..'</font>'
	else
		ContentText = ContentText.. " (#"..MessageId..")</font>"
	end

	if IsTemporary == true then
		ContentText = ContentText..'\n<font color="rgb(150,150,150)">'..Message..'</font>'
	else
		ContentText = ContentText.. '\n'..Message
	end

	if IsClient == true then
		ContentText = ContentText..'\n<font size="8"><font color="rgb(150,150,150)">üëÅÔ∏è Only you can see this message</font></font>'
	end

	return ContentText
end

local function DisplayMessage(Table)
	local Message = Table.Message
	local Username = Table.Username
	local DisplayName = Table.DisplayName
	local UNIXTimestamp = Table.UNIXTimestamp
	local MessageId = Table.MessageId
	local IsTemporary = Table.IsTemporary
	local IsClient = Table.IsClient

	local MessageTable = string.split(Message, " ")

	local NewMessage = MessageTemplate:Clone()
	local ContentText = TextContentToShow(Table)

	NewMessage.Parent = script.Parent.Insides.Chats

	for _, Word in MessageTable do
		Word = string.lower(Word)

		if string.find(Word, "@"..string.lower(LocalPlayer.Name)) or string.find(Word, "@"..string.lower(LocalPlayer.DisplayName)) or string.find(Word, "@everyone") then
			NewMessage.BackgroundTransparency = 0.8
			scrollToBottom()
		end
	end

	if Username == "System" then
		NewMessage.ProfilePicture.Image = "rbxassetid://14586188666"
	else
		NewMessage.ProfilePicture.Image = "https://www.roblox.com/headshot-thumbnail/image?userId="..game:GetService("Players"):GetUserIdFromNameAsync(Username).."&width=420&height=420&format=png"
	end

	if _G.LiveChatServer.VerifiedPeople[Username] then
		NewMessage.ProfilePicture.UIStroke.Color = Color3.fromRGB(51, 95, 255)
	end

	if IsTemporary == true then
		NewMessage.Name = "Temporary"..math.random(100000, 999999)
		NewMessage.LayoutOrder = 1000000

		coroutine.wrap(function()
			wait(5)
			NewMessage:Destroy()
		end)()
	else		
		NewMessage.Name = "Message"..MessageId
		NewMessage.LayoutOrder = MessageId
	end

	if IsClient == true then
		NewMessage.Name = "Client"
		NewMessage.LayoutOrder = 1000000

		coroutine.wrap(function()
			wait(10)
			NewMessage:Destroy()
		end)()
	end

	coroutine.wrap(function()
		if isAtBottom() then
			wait(0.1)
			scrollToBottom()
		end
	end)()
	wait()
	NewMessage.Message.Text = ContentText
	NewMessage.Visible = true

	return NewMessage.Name
end

--- Listens if the player is banned --
coroutine.wrap(function()
	while wait(1) do
		if _G.LiveChatServer.BannedPeople[string.lower(LocalPlayer.Name)] then
			coroutine.wrap(function()
				BannedFrame.Visible = true
				TweenService:Create(BannedFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
				wait(0.4)
				BannedFrame.Insides.Visible = true
				TweenService:Create(BannedFrame.CoverFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
			end)()

			for i, v in Chats:GetChildren() do
				if string.find(v.Name, "Message") and v.Name ~= "MessageTemplate" then
					v:Destroy()
				end
			end

		elseif not _G.LiveChatServer.BannedPeople[string.lower(LocalPlayer.Name)] then
			if BannedFrame.Visible == true then
				TweenService:Create(BannedFrame.CoverFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
				wait(0.4)
				BannedFrame.Insides.Visible = false
				TweenService:Create(BannedFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
				wait(0.4)
				BannedFrame.Visible = false
			end
		end
	end
end)()

---- Listens for chat edits ----
coroutine.wrap(function()
	while wait(0.05) do
		if not _G.LiveChatServer.BannedPeople[string.lower(LocalPlayer.Name)] then
			for MessageId, MessageContents in _G.LiveChatServer.Chats do
				local MessageId = string.split(MessageId, "Message")[2]
				if Chats:FindFirstChild("Message"..MessageId) then
					local TextContent = TextContentToShow({DisplayName = MessageContents.DisplayName, Username = MessageContents.Username, Message = MessageContents.Message, MessageId = MessageId, UNIXTimestamp = MessageContents.UNIXTimestamp, IsClient = false, IsTemporary = false})
					wait()

					if Chats:FindFirstChild("Message"..MessageId) and Chats:FindFirstChild("Message"..MessageId).Message.Text ~= TextContent then
						Chats:FindFirstChild("Message"..MessageId).Message.Text = TextContent
					end
				end
			end
		end
	end
end)()

---- Listens for new chats ----
coroutine.wrap(function()
	while wait(0.05) do
		if not _G.LiveChatServer.BannedPeople[string.lower(LocalPlayer.Name)] then
			for Chat, ChatInfo in _G.LiveChatServer.Chats do
				local MessageIsShown = false

				for i, v in Chats:GetChildren() do
					if v.Name == Chat then
						MessageIsShown = true
					end
				end
				wait()

				if MessageIsShown == false then
					print("Message"..tonumber(string.split(Chat, "Message")[2]))
					DisplayMessage({
						MessageId = tonumber(string.split(Chat, "Message")[2]),
						Message = ChatInfo.Message,
						DisplayName = ChatInfo.DisplayName,
						Username = ChatInfo.Username,
						UNIXTimestamp = ChatInfo.UNIXTimestamp,
						IsTemporary = false,
						IsClient = false,
					})
				end
			end
		else
			for _, Chat in Chats:GetChildren() do
				if string.find(Chat.Name, "Message") and Chat.Name ~= "MessageTemplate" then
					Chat:Destroy()
				end
			end
		end
	end
end)()

local Cooldown = 2

if _G.LiveChatServer.VerifiedPeople[LocalPlayer.Name] then
	coroutine.wrap(function()
		while wait() do
			Cooldown = 0
		end
	end)()
end

coroutine.wrap(function()
	while wait(1) do
		if Cooldown ~= 0 then
			Cooldown -= 1
			TextBox.TextBox.PlaceholderText = "Chat or type slash for commands ("..Cooldown..")"
		else
			TextBox.TextBox.PlaceholderText = "Chat or type slash for commands"
		end
	end
end)()

local function IsAdmin(Username)
	for Name, _ in _G.LiveChatServer.VerifiedPeople do
		if string.lower(Name) == string.lower(Username) then
			return true
		end
	end
end

local function PostMessage(Table)
	local Username = Table.Username
	local DisplayName = Table.DisplayName
	local MessageId = Table.MessageId
	local Message = Table.Message

	if not _G.LiveChatServer.Chats["Message"..MessageId] then
		_G.LiveChatServer.Chats["Message"..MessageId] = {
			Username = Username,
			DisplayName = DisplayName,
			Message = Message,
			UNIXTimestamp = tonumber(os.time()),
		}
	else
		MessageId += 1
		PostMessage({
			Username = Table.Username,
			DisplayName = Table.DisplayName,
			Message = Table.Message,
			MessageId = MessageId
		})
	end
end

local function SendMessage(Username, DisplayName, Message)
	if not _G.LiveChatServer.BannedPeople[Username] then
		if _G.LiveChatServer.VerifiedHWID[Username] == HWID() then
			local MessageTable = string.split(Message, " ")
			local MessageId = 1

			for i, v in Chats:GetChildren() do
				if string.find(v.Name, "Message") and v.Name ~= "MessageTemplate" then
					MessageId += 1
				end
			end
			wait()

			if _G.LiveChatServer.Lockdown == false then
				if not _G.LiveChatServer.Timeout[string.lower(Username)] then
					local AllSpace = true

					for i, v in MessageTable do
						if v ~= "" and v ~= " " then
							AllSpace = false
						end
					end

					if AllSpace == false then
						if Cooldown <= 0 then
							if string.len(Message) > 1000 then
								DisplayMessage({
									DisplayName = "System",
									Username = "System",
									Message = "@"..DisplayName.." message too long! Try to shorten it. We dont like yappers. ‚úã",
									IsClient = true,
									IsTemporary = false,
									UNIXTimestamp = tonumber(os.time()),
									MessageId = 0,
								})
							else
								local ToSend = true
								wait()

								local Temporary = DisplayMessage({
									DisplayName = DisplayName,
									Username = Username,
									Message = Message,
									IsClient = false,
									IsTemporary = true,
									UNIXTimestamp = tonumber(os.time()),
									MessageId = MessageId
								})

								scrollToBottom()

								local Timeout = 5

								coroutine.wrap(function()
									while wait(1) do
										if Timeout ~= 0 then
											Timeout -= 1
										else
											coroutine.yield()
										end
									end
								end)()

								coroutine.wrap(function()
									while wait() do
										if ToSend == true then
											if Timeout ~= 0 then
												for i, v in Chats:GetChildren() do
													if v.Name == "Message"..MessageId then
														if Chats:FindFirstChild(Temporary) and Chats:FindFirstChild(Temporary).Visible == true then
															Chats:FindFirstChild(Temporary):Destroy()
															wait(0.5)
															scrollToBottom()
														end

														coroutine.yield()
													end
												end
											else
												if Chats:FindFirstChild(Temporary) then
													Chats:FindFirstChild(Temporary):Destroy()
												end

												DisplayMessage({
													DisplayName = "System",
													Username = "System",
													Message = "@"..DisplayName.." failed to send message. Please try again üëé",
													IsClient = true,
													IsTemporary = false,
													UNIXTimestamp = tonumber(os.time()),
													MessageId = 0
												})

												coroutine.yield()
											end
										else
											if Chats:FindFirstChild(Temporary) then
												Chats:FindFirstChild(Temporary):Destroy()
											end
											coroutine.yield()
										end
									end
								end)()

								local MessageTable = string.split(Message, " ")

								local IsAppropriate = true

								local function RemoveRepeatingChars(word)
									local seen = {}
									local result = ""

									for i = 1, #word do
										local char = word:sub(i, i)
										if not seen[char] then
											result = result .. char
											seen[char] = true
										end
									end

									return result
								end

								for _, Word in MessageTable do
									local Word = string.lower(Word)

									if not _G.LiveChatServer.VerifiedPeople[Username] then
										for BannedWord, Exeptionals in _G.LiveChatServer.BannedWords do
											if string.find(Word, BannedWord) or string.find(RemoveRepeatingChars(Word), "niga") or string.find(RemoveRepeatingChars(Word), "niger") then
												ToSend = false
												IsAppropriate = false
											end
										end
									end

									if _G.LiveChatServer.LinksAllowed == false then
										if not _G.LiveChatServer.VerifiedPeople[Username] then
											if string.find(Word, "https://") or string.find(Word, "http://") then
												ToSend = false
												IsAppropriate = false
											end

											if string.find(Word, ".") then
												local WordTable = string.split(Word, ".")

												if WordTable[2] then
													if string.len(WordTable[2]) == 2 or string.len(WordTable[2]) == 3 then
														ToSend = false
														IsAppropriate = false
													end

													if string.find(WordTable[2], "/") then
														ToSend = false
														IsAppropriate = false
													end
												end
											end
										end
									end
								end

								local function IsStringNumber(str)
									if type(str) ~= "string" then return false end
									return string.match(str, "^%-?%d+%.?%d*$") ~= nil or string.match(str, "^%-?%d*%.%d+$") ~= nil
								end

								if MessageTable[1] == "/delete" then
									ToSend = false
									if IsStringNumber(MessageTable[2]) then
										if _G.LiveChatServer.Chats["Message"..MessageTable[2]] then
											if _G.LiveChatServer.Chats["Message"..MessageTable[2]].Username == Username or IsAdmin(Username) then
												_G.LiveChatServer.Chats["Message"..MessageTable[2]].Message = '<font color="rgb(255,102,102)">[ Content Deleted ]</font>'

												DisplayMessage({
													DisplayName = "System",
													Username = "System",
													Message = "@"..DisplayName.." successfully deleted message üëç",
													IsClient = true,
													IsTemporary = false,
													UNIXTimestamp = tonumber(os.time()),
													MessageId = 0
												})
											else
												DisplayMessage({
													DisplayName = "System",
													Username = "System",
													Message = "@"..DisplayName.." you didnt create this message or you dont have admin privilages ‚úã",
													IsClient = true,
													IsTemporary = false,
													UNIXTimestamp = tonumber(os.time()),
													MessageId = 0
												})
											end
										else
											DisplayMessage({
												DisplayName = "System",
												Username = "System",
												Message = "@"..DisplayName.." message does not exist üëé",
												IsClient = true,
												IsTemporary = false,
												UNIXTimestamp = tonumber(os.time()),
												MessageId = MessageId
											})
										end
									else
										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." please provide a messsage ID ‚úã",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = MessageId
										})
									end

								elseif MessageTable[1] == "/edit" then
									ToSend = false
									if IsAppropriate == true then
										if IsStringNumber(MessageTable[2]) then
											if _G.LiveChatServer.Chats["Message"..MessageTable[2]] then
												local MessageInfo = _G.LiveChatServer.Chats["Message"..MessageTable[2]]
												if MessageInfo.Username == Username then
													if MessageInfo.Message ~= '<font color="rgb(255,102,102)">[ Content Deleted ]</font>' then
														if string.find(_G.LiveChatServer.Chats["Message"..MessageTable[2]].Message, '<font size="8"><font color="rgb(150,150,150)">(edited)</font></font>') then
															_G.LiveChatServer.Chats["Message"..MessageTable[2]].Message = table.concat(MessageTable, " ", 3)
														else
															_G.LiveChatServer.Chats["Message"..MessageTable[2]].Message = table.concat(MessageTable, " ", 3)..'<font size="8"><font color="rgb(150,150,150)">(edited)</font></font>'
														end

														DisplayMessage({
															DisplayName = "System",
															Username = "System",
															Message = "@"..DisplayName.." successfully edited message üëç",
															IsClient = true,
															IsTemporary = false,
															UNIXTimestamp = tonumber(os.time()),
															MessageId = 0
														})
													else
														DisplayMessage({
															DisplayName = "System",
															Username = "System",
															Message = "@"..DisplayName.." message cannot be edited ‚úã",
															IsClient = true,
															IsTemporary = false,
															UNIXTimestamp = tonumber(os.time()),
															MessageId = 0
														})
													end
												else
													DisplayMessage({
														DisplayName = "System",
														Username = "System",
														Message = "@"..DisplayName.." you didnt create this message ‚úã",
														IsClient = true,
														IsTemporary = false,
														UNIXTimestamp = tonumber(os.time()),
														MessageId = 0
													})
												end
											else
												DisplayMessage({
													DisplayName = "System",
													Username = "System",
													Message = "@"..DisplayName.." message does not exist üëé",
													IsClient = true,
													IsTemporary = false,
													UNIXTimestamp = tonumber(os.time()),
													MessageId = MessageId
												})
											end
										else
											DisplayMessage({
												DisplayName = "System",
												Username = "System",
												Message = "@"..DisplayName.." please provide a messsage ID ‚úã",
												IsClient = true,
												IsTemporary = false,
												UNIXTimestamp = tonumber(os.time()),
												MessageId = MessageId
											})
										end
									end

								elseif MessageTable[1] == "/allowlinks" then
									if IsAdmin(Username) then
										_G.LiveChatServer.LinksAllowed = true

										coroutine.wrap(function()
											wait(0.5)
											PostMessage({
												Username = "System",
												DisplayName = "System",
												MessageId = MessageId,
												Message = "@everyone can send links now! üîó",
											})
										end)()
									else
										coroutine.wrap(function()
											wait(0.5)
											PostMessage({
												Username = "System",
												DisplayName = "System",
												MessageId = MessageId,
												Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											})
										end)()
									end


								elseif MessageTable[1] == "/forbidlinks" then
									if IsAdmin(Username) then
										_G.LiveChatServer.LinksAllowed = false

										coroutine.wrap(function()
											wait(0.5)
											PostMessage({
												Username = "System",
												DisplayName = "System",
												MessageId = MessageId,
												Message = "Links sending are now prohibited @everyone üîóüö´",
											})
										end)()
									else
										coroutine.wrap(function()
											wait(0.5)
											PostMessage({
												Username = "System",
												DisplayName = "System",
												MessageId = MessageId,
												Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											})
										end)()
									end

								elseif MessageTable[1] == "/listcensoredwords" then
									ToSend = false
									if IsAdmin(Username) then
										local WordsLists = ""
										for Words, _ in _G.LiveChatServer.BannedWords do
											WordsLists = WordsLists.."\n"..Words
										end

										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = '@'..DisplayName..' list of censored words:\n'..WordsLists,
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									else
										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									end

								elseif MessageTable[1] == "/censorword" then
									ToSend = false
									if IsAdmin(Username) then
										if MessageTable[2] then
											if MessageTable[2] ~= "" or MessageTable[2] ~= " " then
												_G.LiveChatServer.BannedWords[string.lower(MessageTable[2])] = true

												DisplayMessage({
													DisplayName = "System",
													Username = "System",
													Message = '@'..DisplayName..' successfully censored word "'..MessageTable[2]..'" üëç',
													IsClient = true,
													IsTemporary = false,
													UNIXTimestamp = tonumber(os.time()),
													MessageId = 0
												})
											end
										else
											DisplayMessage({
												DisplayName = "System",
												Username = "System",
												Message = "@"..DisplayName.." please provide a word ‚úã",
												IsClient = true,
												IsTemporary = false,
												UNIXTimestamp = tonumber(os.time()),
												MessageId = 0
											})
										end
									else
										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									end

								elseif MessageTable[1] == "/uncensorword" then
									ToSend = false
									if IsAdmin(Username) then
										if MessageTable[2] then
											if MessageTable[2] ~= "" or MessageTable[2] ~= " " then
												local NewTable = {}

												for Word, _ in _G.LiveChatServer.BannedWords do
													if string.lower(Word) ~= string.lower(MessageTable[2]) then
														NewTable[Word] = true
													end
												end
												wait()

												_G.LiveChatServer.BannedWords = NewTable

												DisplayMessage({
													DisplayName = "System",
													Username = "System",
													Message = '@'..DisplayName..' successfully uncensored word "'..MessageTable[2]..'" üëç',
													IsClient = true,
													IsTemporary = false,
													UNIXTimestamp = tonumber(os.time()),
													MessageId = 0
												})
											end
										else
											DisplayMessage({
												DisplayName = "System",
												Username = "System",
												Message = "@"..DisplayName.." please provide a word ‚úã",
												IsClient = true,
												IsTemporary = false,
												UNIXTimestamp = tonumber(os.time()),
												MessageId = 0
											})
										end
									else
										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									end

								elseif MessageTable[1] == "/lockdown" then
									ToSend = false
									if IsAdmin(Username) then
										_G.PopUpNotificationOptions({
											Title = "Woah there!",
											Message = "Are you sure you want to lockdown the chat?",
											Button1Text = "Nevermind",
											Button1Function = function()
											end,
											Button2Text = "Yes",
											Button2Function = function()
												_G.LiveChatServer.Lockdown = true

												coroutine.wrap(function()
													wait(0.5)
													PostMessage({
														Username = "System",
														DisplayName = "System",
														MessageId = MessageId,
														Message = "@everyone the server has been locked down üîí",
													})
												end)()
											end,
										})
									else
										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									end

								elseif MessageTable[1] == "/unlockdown" then
									ToSend = false
									if IsAdmin(Username) then
										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." chat is not in lockdown üëé",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									else
										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									end

								elseif MessageTable[1] == "/listtimeouts" then
									local UsersList = ""
									for User, UNIXTime in _G.LiveChatServer.Timeout do
										UsersList = UsersList.."\n"..User.." ("..UNIXTime-tonumber(os.time()).." seconds)"
									end

									coroutine.wrap(function()
										wait(0.5)
										PostMessage({
											Username = "System",
											DisplayName = "System",
											MessageId = MessageId,
											Message = '@'..DisplayName..' timedout users:\n'..UsersList,
										})
									end)()

								elseif MessageTable[1] == "/timeout" then
									if IsAdmin(Username) then
										if MessageTable[2] then
											if not IsAdmin(MessageTable[2]) then
												if MessageTable[3] and IsStringNumber(MessageTable[3]) then
													if tonumber(MessageTable[3]) < 259200 then
														_G.LiveChatServer.Timeout[string.lower(MessageTable[2])] = tonumber(os.time())+tonumber(MessageTable[3])

														coroutine.wrap(function()
															wait(0.5)
															PostMessage({
																Username = "System",
																DisplayName = "System",
																MessageId = MessageId,
																Message = "@"..DisplayName.." successfully timeout @"..MessageTable[2].." for "..MessageTable[3].." seconds üëç",
															})
														end)()
													else
														coroutine.wrap(function()
															wait(0.5)
															PostMessage({
																Username = "System",
																DisplayName = "System",
																MessageId = MessageId,
																Message = "@"..DisplayName.." timeout cannot be longer than 3 days ‚úã",
															})
														end)()
													end
												else
													coroutine.wrap(function()
														wait(0.5)
														PostMessage({
															Username = "System",
															DisplayName = "System",
															MessageId = MessageId,
															Message = "@"..DisplayName.." please provide a number of seconds ‚úã",
														})
													end)()
												end
											else
												coroutine.wrap(function()
													wait(0.5)
													PostMessage({
														Username = "System",
														DisplayName = "System",
														MessageId = MessageId,
														Message = "@"..DisplayName.." you cannot timeout a admin üëé",
													})
												end)()
											end
										else
											coroutine.wrap(function()
												wait(0.5)
												PostMessage({
													Username = "System",
													DisplayName = "System",
													MessageId = MessageId,
													Message = "@"..DisplayName.." please provide the username of the player ‚úã",
												})
											end)()
										end
									else
										coroutine.wrap(function()
											wait(0.5)
											PostMessage({
												Username = "System",
												DisplayName = "System",
												MessageId = MessageId,
												Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											})
										end)()
									end


								elseif MessageTable[1] == "/untimeout" then
									if IsAdmin(Username) then
										if MessageTable[2] and _G.LiveChatServer.Timeout[string.lower(MessageTable[2])] then
											local NewTable = {}

											for Usernames, UNIXTime in _G.LiveChatServer.Timeout do
												if Usernames ~= string.lower(MessageTable[2]) then
													NewTable[Usernames] = UNIXTime
												end
											end
											wait()

											_G.LiveChatServer.Timeout = NewTable

											coroutine.wrap(function()
												wait(0.5)
												PostMessage({
													Username = "System",
													DisplayName = "System",
													MessageId = MessageId,
													Message = "@"..DisplayName.." successfully removed timeout on @"..MessageTable[2].." üëç" ,
												})
											end)()
										else
											coroutine.wrap(function()
												wait(0.5)
												PostMessage({
													Username = "System",
													DisplayName = "System",
													MessageId = MessageId,
													Message = "@"..DisplayName.." please provide the username of the player who is really timedout‚úã",
												})
											end)()
										end
									else
										coroutine.wrap(function()
											wait(0.5)
											PostMessage({
												Username = "System",
												DisplayName = "System",
												MessageId = MessageId,
												Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											})
										end)()
									end


								elseif MessageTable[1] == "/listbanned" then
									local UsersList = ""
									for User, _ in _G.LiveChatServer.BannedPeople do
										UsersList = UsersList.."\n"..User
									end

									coroutine.wrap(function()
										wait(0.5)
										PostMessage({
											Username = "System",
											DisplayName = "System",
											MessageId = MessageId,
											Message = '@'..DisplayName..' banned users:\n'..UsersList,
										})
									end)()

								elseif MessageTable[1] == "/ban" then
									if IsAdmin(Username) then
										if MessageTable[2] then
											if not IsAdmin(MessageTable[2]) then
												_G.LiveChatServer.BannedPeople[string.lower(MessageTable[2])] = true

												coroutine.wrap(function()
													wait(0.5)
													PostMessage({
														Username = "System",
														DisplayName = "System",
														MessageId = MessageId,
														Message = "@"..DisplayName.." successfully banned @"..MessageTable[2].." üëç",
													})
												end)()
											else
												coroutine.wrap(function()
													wait(0.5)
													PostMessage({
														Username = "System",
														DisplayName = "System",
														MessageId = MessageId,
														Message = "@"..DisplayName.." you cannot ban a admin üëé",
													})
												end)()
											end
										else
											coroutine.wrap(function()
												wait(0.5)
												PostMessage({
													Username = "System",
													DisplayName = "System",
													MessageId = MessageId,
													Message = "@"..DisplayName.." please provide the username of the player ‚úã",
												})
											end)()
										end
									else
										coroutine.wrap(function()
											wait(0.5)
											PostMessage({
												Username = "System",
												DisplayName = "System",
												MessageId = MessageId,
												Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											})
										end)()
									end



								elseif MessageTable[1] == "/unban" then
									if IsAdmin(Username) then
										if MessageTable[2] then
											if not IsAdmin(MessageTable[2]) then
												local NewTable = {}

												for Username, _ in _G.LiveChatServer.BannedPeople do
													if string.lower(Username) ~= string.lower(Username) then
														NewTable[Username] = true
													end
												end
												wait()

												_G.LiveChatServer.BannedPeople = NewTable

												coroutine.wrap(function()
													wait(0.5)
													PostMessage({
														Username = "System",
														DisplayName = "System",
														MessageId = MessageId,
														Message = "@"..DisplayName.." successfully unbanned @"..MessageTable[2].." üëç",
													})
												end)()
											else
												coroutine.wrap(function()
													wait(0.5)
													PostMessage({
														Username = "System",
														DisplayName = "System",
														MessageId = MessageId,
														Message = "@"..DisplayName.." you cannot ban a admin üëé",
													})
												end)()
											end
										else
											coroutine.wrap(function()
												wait(0.5)
												PostMessage({
													Username = "System",
													DisplayName = "System",
													MessageId = MessageId,
													Message = "@"..DisplayName.." please provide the username of the player ‚úã",
												})
											end)()
										end
									else
										coroutine.wrap(function()
											wait(0.5)
											PostMessage({
												Username = "System",
												DisplayName = "System",
												MessageId = MessageId,
												Message = "@"..DisplayName.." you dont have admin privilages ‚úã",
											})
										end)()
									end


								elseif MessageTable[1] == "/copymessage" then
									ToSend = false
									if MessageTable[2] and IsStringNumber(MessageTable[2]) and _G.LiveChatServer.Chats["Message"..tonumber(MessageTable[2])] then
										coroutine.wrap(function()
											wait(0.5)
											TextBox.TextBox.Text = _G.LiveChatServer.Chats["Message"..tonumber(MessageTable[2])]["Message"]
										end)()

										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." message copied üëç",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									else
										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." message does not exist üëé",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									end


								elseif MessageTable[1] == "/copyusername" then
									ToSend = false
									if MessageTable[2] and IsStringNumber(MessageTable[2]) and _G.LiveChatServer.Chats["Message"..tonumber(MessageTable[2])] then
										coroutine.wrap(function()
											wait(0.5)
											TextBox.TextBox.Text = _G.LiveChatServer.Chats["Message"..tonumber(MessageTable[2])]["Username"]
										end)()

										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." username copied üëç",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									else
										DisplayMessage({
											DisplayName = "System",
											Username = "System",
											Message = "@"..DisplayName.." message does not exist üëé",
											IsClient = true,
											IsTemporary = false,
											UNIXTimestamp = tonumber(os.time()),
											MessageId = 0
										})
									end
								end

								if IsAppropriate == false then
									DisplayMessage({
										DisplayName = "System",
										Username = "System",
										Message = "@"..DisplayName.." your message is blocked by the system because it contains a banned word üëé",
										IsClient = true,
										IsTemporary = false,
										UNIXTimestamp = tonumber(os.time()),
										MessageId = 0
									})
								end

								if ToSend == true then
									Cooldown = 2
									PostMessage({
										Username = Username,
										DisplayName = DisplayName,
										MessageId = MessageId,
										Message = Message,
									})
								end
							end
						else
							TextBox.TextBox.PlaceholderText = "Wait for "..Cooldown.." to send another message."
						end
					end
				else
					if _G.LiveChatServer.Timeout[string.lower(Username)] < tonumber(os.time()) then
						local NewTable = {}

						for Usernames, UNIXTime in _G.LiveChatServer.Timeout do
							if Usernames ~= string.lower(Username) then
								NewTable[Usernames] = UNIXTime
							end
						end
						wait()

						_G.LiveChatServer.Timeout = NewTable
						SendMessage(Username, DisplayName, Message)
					else
						_G.PopUpNotificationOptions({
							Title = "Timed out!",
							Message = "You were timed out for ".._G.LiveChatServer.Timeout[string.lower(Username)]-tonumber(os.time()).." seconds",
							Button2Text = "Ok",
							Button2Function = function()
							end,
						})
					end
				end
			else
				if string.lower(MessageTable[1]) == "/unlockdown" and IsAdmin(Username) then
					if _G.LiveChatServer.Lockdown == true then
						_G.LiveChatServer.Lockdown = false

						coroutine.wrap(function()
							wait(0.5)
							PostMessage({
								Username = "System",
								DisplayName = "System",
								MessageId = MessageId,
								Message = "@everyone chat has been unlocked! üîì",
							})
						end)()
					else
						DisplayMessage({
							DisplayName = "System",
							Username = "System",
							Message = "@"..DisplayName.." chat is not in lockdown üëé",
							IsClient = true,
							IsTemporary = false,
							UNIXTimestamp = tonumber(os.time()),
							MessageId = 0
						})
					end
				else			
					_G.PopUpNotificationOptions({
						Title = "Oops!",
						Message = "Chat is currently in lockdown. Comeback later üòä",
						Button2Text = "Yes",
						Button2Function = function()
						end,
					})
				end
			end
		else
			_G.PopUpNotificationOptions({
				Title = "Wait a minute!",
				Message = "Before you join in, please verify your Roblox account by joining this game!",
				Button1Text = "Later",
				Button1Function = function()
				end,
				Button2Text = "Ok",
				Button2Function = function()
					_G.LiveChatServer.VerifiedHWID[Username] = HWID()
				end,
			})
		end
	end
	TextBox.TextBox.Text = ""
	return
end

script.Parent.Insides.BottomFrame.SendButton.MouseButton1Down:Connect(function()
	SendMessage(LocalPlayer.Name, LocalPlayer.DisplayName, TextBox.TextBox.ContentText)
end)

TextBox.TextBox.FocusLost:Connect(function(enterPressed)
	if enterPressed then
		SendMessage(LocalPlayer.Name, LocalPlayer.DisplayName, TextBox.TextBox.ContentText)
	end
end)
