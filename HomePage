-- Scripts:

local Announcement = "no"
local Updates = {
	"fnof",
	"fubbia"
}

local IsInAnnouncement = nil

function ShowAnnouncements()
	IsInAnnouncement = true
	AnnouncementsOrUpdates.Text = '<b><font size="14">Announcements:</font></b>\n'..Announcement
	AnnouncementButton.ImageColor3 = Color3.fromRGB(255, 255, 255)
	UpdatesButton.ImageColor3 = Color3.fromRGB(190, 190, 190)
end

function ShowUpdates()
	IsInAnnouncement = false
	AnnouncementsOrUpdates.Text = '<b><font size="14">Updates:</font></b>\n'
	AnnouncementButton.ImageColor3 = Color3.fromRGB(190, 190, 190)
	UpdatesButton.ImageColor3 = Color3.fromRGB(255, 255, 255)
	for i, v in Updates do
		AnnouncementsOrUpdates.Text = AnnouncementsOrUpdates.Text..""..v.."\n"
	end
end

AnnouncementButton.MouseButton1Down:Connect(function()
	ShowAnnouncements()
end)

UpdatesButton.MouseButton1Down:Connect(function()
	ShowUpdates()
end)

AnnouncementButton.MouseEnter:Connect(function()
	if IsInAnnouncement == false then
		AnnouncementButton.ImageColor3 = Color3.fromRGB(255, 255, 255)
	end
end)

AnnouncementButton.MouseLeave:Connect(function()
	if IsInAnnouncement == false then
		AnnouncementButton.ImageColor3 = Color3.fromRGB(190, 190, 190)
	end
end)

UpdatesButton.MouseEnter:Connect(function()
	if IsInAnnouncement == true then
		UpdatesButton.ImageColor3 = Color3.fromRGB(255, 255, 255)
	end
end)

UpdatesButton.MouseLeave:Connect(function()
	if IsInAnnouncement == true then
		UpdatesButton.ImageColor3 = Color3.fromRGB(190, 190, 190)
	end
end)

local RandomNumber = math.random(1, 3)

if RandomNumber == 1 then
	ShowAnnouncements()
elseif RandomNumber == 2 then
	ShowUpdates()
elseif RandomNumber == 3 then
	AnnouncementsOrUpdates.Text = "Click the megaphone to see announcements\n\nor\n\nClick the paper with a pencil to see updates"
end

if tonumber(os.date("%H")) >= 0 and tonumber(os.date("%H")) <= 5 then
	WelcomeText.Text = 'Good Evening, <b>'..LocalPlayer.DisplayName..'!</b>'
elseif tonumber(os.date("%H")) >= 6 and tonumber(os.date("%H")) <= 11 then
	WelcomeText.Text = 'Good Morning, <b>'..LocalPlayer.DisplayName..'!</b>'
elseif tonumber(os.date("%H")) >= 12 and tonumber(os.date("%H")) <= 17 then
	WelcomeText.Text = 'Good Afternoon, <b>'..LocalPlayer.DisplayName..'!</b>'
elseif tonumber(os.date("%H")) >= 18 and tonumber(os.date("%H")) <= 23 then
	WelcomeText.Text = 'Good Evening, <b>'..LocalPlayer.DisplayName..'!</b>'
end
