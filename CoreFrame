local TweenService = game:GetService("TweenService")

function CreateTab(TabName, TabImage, TabFunction)
	local Clone = TabTemplate:Clone()
	
	Clone.Name = TabName
	Clone.Parent = TabChoices
	Clone.Visible = true
	Clone.TabName.Text = TabName
	Clone.TabImage.Image = TabImage
	Clone.MouseButton1Click:Connect(function()
		pcall(TabFunction)
	end)
	
	Clone.MouseEnter:Connect(function()
		Clone.TabImage.ImageColor3 = Color3.fromRGB(255, 255, 255)
	end)
	
	Clone.MouseLeave:Connect(function()
		Clone.TabImage.ImageColor3 = Color3.fromRGB(190, 190, 190)
	end)
end

function CreateSectionName(SectionName)
	local Clone = SectionsNameTemplate:Clone()
	
	Clone.Name = SectionName
	Clone.Parent = TabChoices
	Clone.Visible = true
	Clone.Text = SectionName
end

CloseButton.MouseButton1Down:Connect(function()
	Ads.Visible = false
end)

Close.MouseEnter:Connect(function()
	Close.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
	BottomFrame.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
end)

Close.MouseLeave:Connect(function()
	Close.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
	BottomFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
end)

Close.MouseButton1Down:Connect(function()
	TweenService:Create(CoverFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {BackgroundTransparency = 0}):Play()
	TweenService:Create(Ads, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(0, 0, 0.798, 0)}):Play()
	TweenService:Create(Close, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(0.92, 0, 0, 0)}):Play()
	wait(0.4)
	MainContents.Visible = false
	Ads.Visible = false
	Close.Visible = false
	TweenService:Create(CoverFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
	TweenService:Create(UIScale, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Scale = 0}):Play()
	TweenService:Create(UICorner_6, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {CornerRadius = UDim.new(0, 50)}):Play()
	wait(0.6)
	DragFrame.Visible = false
	
	wait(3)
	DragFrame.Visible = true
	Ads.Visible = true
	Close.Visible = true
	MainContents.Visible = true
	CoverFrame.BackgroundTransparency = 0
	TweenService:Create(CoverFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
	TweenService:Create(UIScale, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Scale = 1}):Play()
	TweenService:Create(UICorner_6, TweenInfo.new(0.2, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {CornerRadius = UDim.new(0, 5)}):Play()
	TweenService:Create(Ads, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Position = UDim2.new(0, 0, 1.02999997, 0)}):Play()
	TweenService:Create(Close, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Position = UDim2.new(0.920000017, 0, -0.119999997, 0)}):Play()
end)

local NumberOfAds = 0
local AdsInNumber = {}
local AdsTable = {
	sakjboaubfoi = {
		AdImage = "rbxassetid://11592824883",
		AdCreator = "gbswg",
		AdWhenClicked = "",
		AdViews = {},
		AdClicks = {},
		AdExpiration = "01/10/2025"
	},
	
	sajboaubfoi = {
		AdImage = "rbxassetid://11926598891",
		AdCreator = "sgt",
		AdWhenClicked = "",
		AdViews = {},
		AdClicks = {},
		AdExpiration = "01/10/2025"
	}
}

for i, v in AdsTable do
	table.insert(AdsInNumber, v)
	NumberOfAds += 1
end

local AdToShow = AdsInNumber[math.random(1, NumberOfAds)]

Ads.Image = AdToShow.AdImage
AdText.Text = 'Ad <font size="8">(by @'..AdToShow.AdCreator..')</font>'

CreateTab("Required Accessories", "rbxassetid://11592824883")
